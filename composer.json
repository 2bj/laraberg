{
    "name": "mauricewijnia/laraberg",
    "description": "A Gutenberg implementation for Laravel",
    "type": "project",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Maurice Wijnia",
            "email": "mauricewijnia@live.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "embed/embed": "^3.3",
        "unisharp/laravel-filemanager": "~1.8"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "MauriceWijnia\\Laraberg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MauriceWijnia\\Laraberg\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": "MauriceWijnia\\Laraberg\\LarabergServiceProvider"
        }
    }

}
